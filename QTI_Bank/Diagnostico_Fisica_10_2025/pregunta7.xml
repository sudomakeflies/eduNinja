            <simpleChoice  is_latex="False"  indetifier="ChoiceB">20 m/s</simpleChoice>
            <simpleChoice  is_latex="False"  indetifier="ChoiceC">72 m/s</simpleChoice>
            <simpleChoice  is_latex="False"  indetifier="ChoiceD">200 m/s</simpleChoice>
        </choiceInteraction>
