            <simpleChoice is_latex="False" identifier="ChoiceA" image="EA_MAT_11_2023_1_p10a.png"></simpleChoice>
            <simpleChoice is_latex="False" identifier="ChoiceB" image="EA_MAT_11_2023_1_p10b.png"></simpleChoice>
            <simpleChoice is_latex="False" identifier="ChoiceC" image="EA_MAT_11_2023_1_p10c.png"></simpleChoice>
            <simpleChoice is_latex="False" identifier="ChoiceD" image="EA_MAT_11_2023_1_p10d.png"></simpleChoice>
